    "f(t, y) = sin(t)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "function euler(tn, yn, f, h)\n",
    "    return yn + h*f(t, yn)\n",
    "end\n",
    "\n",
    "t, yn = 0, 0\n",
    "h = 0.1 \n",
    "T, Y = [], []\n",
    "\n",
    "for i in 1:100\n",
    "    yn = euler(t, yn, f, h)\n",
    "    append!(T, t)\n",
    "    append!(Y, yn)\n",
    "    t = t + h\n",
    "end"
   ]
  },
